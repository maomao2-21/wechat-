@import"./styles/iconfont.wxss";

/* 不支持通配符 */

page,
view,
text,
swiper,
swiper-item,
image,
navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}


/* 主题颜色 通过变量实现 color:var(--themeColor) */


/* 定义统一字体大小 */

page {
    --themeColor: #eb4450;
    font-size: 28rpx;
}

image {
    width: 100%;
}